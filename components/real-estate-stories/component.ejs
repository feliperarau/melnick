                            <div class="swiper">
                                <%- include( './swiper-controls.ejs' ); %>
                                <div class="swiper-wrapper">
                                    <div class="swiper-slide">
                                        <%- include( './slide-bairro.ejs', { args: { caption: 'Conheça o Bairro', bg: 'bairro.jpeg' } } ); %>
                                    </div>
                                    <div class="swiper-slide wide">
                                        <%- include( './slide-bairro.ejs', { args: { caption: 'Conheça o Bairro 2', bg: 'office.jpeg' } } ); %>
