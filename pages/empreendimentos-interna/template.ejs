<% 
const realEstates = JSON.parse(include('../../data/real-estates/properties.json') )
%>

<%- include('../../components/header/component.ejs') %>

<main class="site-wrapper" id="page-empreendimentos-interna">
    <div class="page-inner">

        <section class="mobile-gallery">
            <%- include('../../components/real-estate-gallery-mobile/component.ejs' ); -%>
        </section>

        <section class="gallery container-fluid">
            <%- include('../../components/real-estate-gallery/component.ejs', { args: { cssClass: 'mb-99' } } ); -%>
        </section>

        <div class="form-scrollable">
            <section class="meta container-fluid">
                <div class="left">
                    <%- include('../../components/real-estate-meta-actions/component.ejs') %>

                    <%- include('../../components/page-title/component.ejs', { args: { title: 'Nilo Square Residence Resort', description: 'Um lugar assim muda sua vida' } } ); -%>

                    <%- include('../../components/real-estate-props/component.ejs' ); -%>

                    <%- include('../../components/real-estate-action-carousel/component.ejs' ); -%>
                </div>
                <div class="right">
                    <div class="rail">
                        <%- include('../../components/real-estate-form/component.ejs' ); -%>
