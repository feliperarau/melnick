            <section class="testimonies mx-lg-n20">
                <%- include('../../components/real-estate-testimonies/component.ejs' ); -%>
            </section>
        </div>

        <section class="map mx-lg-n20">
            <%- include('../../components/real-estate-map/component.ejs' ); -%>
        </section>

        <section class="related">
            <%- include('../../components/real-estate-related/component.ejs' ); -%>
        </section>
    </div>

    <%- include('../../components/real-estate-modal/component.ejs' ); -%>

